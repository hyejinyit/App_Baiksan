<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"  >
    <!-- 현재작업자 표시 박스 : 검은배경용 -->
    <!-- 평상시 -->
    <item android:state_focused="false" android:state_pressed="false">
        <shape>
            <corners
                android:radius="50dp" />
            <solid
                android:color="#f8f8f8" />
            <size
                android:width="280dp"
                android:height="90dp" />
            <stroke
                android:width="1dp"
                android:color="#3F3F3F"/>
        </shape>
    </item>
</selector>


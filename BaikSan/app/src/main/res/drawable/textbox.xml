<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"  >
    <!-- 바코드 스캔 박스 -->
    <!-- 평상시 -->
    <item android:state_focused="false" android:state_pressed="false">
        <shape>
            <corners
                android:radius="12dp" />
            <solid
                android:color="#F1EFDC" />
            <size
                android:width="280dp"
                android:height="90dp" />
